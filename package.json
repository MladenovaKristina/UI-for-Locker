{
  "name": "Template",
  "version": "2.0.0",
  "description": "Boilerplate for Three.js projects set up with Babel for ES6 and compiled with webpack",
  "author": "AppReal-VR",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "auto": "node autobuilder.js",
    "dev": "run-s dev:js webpack-server",
    "build": "run-s prebuild build:js",
    "prebuild": "run-s clean",
    "clean": "rimraf build",
    "lint": "eslint src/js/app/components/*",
    "webpack-server": "webpack-dev-server --env.NODE_ENV=dev --progress --color --hot --config webpack.dev.config.js",
    "webpack-watch": "webpack --env.NODE_ENV=dev --progress --color --watch --hot",
    "dev:js": "webpack --env.NODE_ENV=dev --config webpack.dev.config.js",
    "build:dir": "copyfiles -a -u 2 src/public/assets/**/*.* build/",
    "build:js": "webpack --config webpack.prod.config.js --env.NODE_ENV=prod --progress --color"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "babel-cli": "^6.26.0",
    "body-scroll-lock": "^2.7.1",
    "cannon": "^0.6.2",
    "compression": "^1.7.4",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "jszip": "^3.9.1",
    "three": "^0.118.3",
    "three-gltf-loader": "1.107.0",
    "three-ik": "^0.1.0",
    "three-text2d": "^0.6.0",
    "threejs-texture-tool": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.16.4",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "babel-preset-babili": "^0.1.4",
    "copyfiles": "^2.3.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "file-loader": "^6.2.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.13.0",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^1.0.0",
    "rimraf": "^2.7.1",
    "style-loader": "^0.23.1",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-json-bundler-plugin": "^1.1.1"
  }
}
